azureAuthentication:
  appId: ""
  appKey: ""

telemetry:
  # Optional
  appInsightsKey: ""

## Follows the format of MetricDeclaration.YAML
azureMetadata:
  tenantId: ""
  subscriptionId: ""
  resourceGroupName: promitor
metricDefaults:
  aggregation:
    interval: 00:05:00
metrics:
  - name: demo_queue_size
    description: "Amount of active messages of the 'myqueue' queue"
    resourceType: ServiceBusQueue
    namespace: promitor-messaging
    queueName: myqueue
    azureMetricConfiguration:
    metricName: ActiveMessages
    aggregation:
        type: Total
        interval: 00:15:00
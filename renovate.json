{
  "masterIssue": false,
  "branchPrefix":"renovate-",
  "separateMajorMinor": false,
  "labels":["dependencies"],
  "assignees": ["@tomkerkhove"],
  "ignoreDeps":[],
  "prHourlyLimit": 0,
  "prConcurrentLimit": 0,
  "packageRules": [
    {
      "datasources": ["docker"],
      "packagePatterns": ["^mcr.microsoft.com/dotnet/core/"],
      "groupName": ".NET Core Docker containers"
    }
  ],
  "extends": [
    "config:base",
    "default:pinDigestsDisabled",
    "default:disablePrControls"
  ],
  "nuget": {
    "enabled": true
   }
}
